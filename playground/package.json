{
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production node build.mjs",
    "dev": "node build.mjs; concurrently 'yarn:dev:* -- --watch'",
    "dev:nodemon": "nodemon --watch src --watch dist --ext js dist/server.js",
    "dev:bundle": "node build.mjs -w",
    "fix": "prettier --write ."
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.10",
    "esbuild-node-externals": "^1.6.0",
    "nodemon": "^2.0.22",
    "tiny-glob": "^0.2.9",
    "watcher": "^2.2.2"
  },
  "dependencies": {
    "@barelyhuman/preact-island-plugins": "workspace:*",
    "express": "^4.18.2",
    "preact": "^10.13.0",
    "preact-render-to-string": "^5.2.6"
  }
}
