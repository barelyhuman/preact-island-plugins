{
  "name": "@barelyhuman/preact-island-plugins",
  "version": "0.0.14-beta.4",
  "repository": "barelyhuman/preact-island-plugins",
  "license": "MIT",
  "author": "reaper <ahoy@barelyhuman.dev>",
  "type": "module",
  "files": [
    "./*"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "tsup; nr copy",
    "copy": "cp package.json dist/ ;cp README.md dist/ ;cp LICENSE dist/",
    "dev": "tsup --watch src",
    "fix": "npx prettier --write .",
    "next": "bumpp",
    "test": "echo 'done'"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,md,json}": [
      "npx prettier --write"
    ]
  },
  "prettier": "@barelyhuman/prettier-config",
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@babel/generator": "^7.21.1",
    "@babel/parser": "^7.21.1",
    "@babel/preset-typescript": "^7.21.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.4",
    "@barelyhuman/prettier-config": "^1.1.0",
    "@types/babel__core": "^7.20.0",
    "@types/node": "^18.14.0",
    "bumpp": "^9.0.0",
    "esbuild": "^0.17.16",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vitest": "^0.32.0"
  },
  "peerDependencies": {
    "esbuild": "^0.17.16",
    "rollup": "^3.17.3",
    "vite": "^4.1.0"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
