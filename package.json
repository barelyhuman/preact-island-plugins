{
  "name": "@barelyhuman/preact-island-plugins",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.js"
    },
    "./esbuild": {
      "types": "./esbuild.d.ts",
      "require": "./esbuild.cjs",
      "import": "./esbuild.js"
    },
    "./rollup": {
      "types": "./rollup.d.ts",
      "require": "./rollup.cjs",
      "import": "./rollup.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*"
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,md,json}": [
      "npx prettier --write"
    ]
  },
  "scripts": {
    "build": "tsup",
    "build:post": "node scripts/postbuild.js",
    "dev": "tsup --watch src",
    "fix": "npx prettier --write ."
  },
  "prettier": "@barelyhuman/prettier-config",
  "dependencies": {
    "@babel/generator": "^7.21.1",
    "@babel/parser": "^7.21.1"
  },
  "devDependencies": {
    "@barelyhuman/prettier-config": "^1.1.0",
    "@types/node": "^18.14.0",
    "fast-glob": "^3.2.12",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}
